#ifndef TOK
#define TOK(ID)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(ID, SP) TOK(ID)
#endif
#ifndef KEYWORD
#define KEYWORD(ID, FLAG) TOK(kw_ ## ID)
#endif

TOK(unknown)             // Not a token.
TOK(eof)                 // End of file.
TOK(identifier)          // abcde123
TOK(integer_literal)     // 123

PUNCTUATOR(bang,            "!")
PUNCTUATOR(colonequal,      ":=")
PUNCTUATOR(closedpar,       ")")
PUNCTUATOR(equal,           "=")
PUNCTUATOR(greater,         ">")
PUNCTUATOR(greatereq,       ">=")
PUNCTUATOR(less,            "<")
PUNCTUATOR(lesseq,          "<=")
PUNCTUATOR(logicand,        "&&")
PUNCTUATOR(logicor,         "||")
PUNCTUATOR(minus,           "-")
PUNCTUATOR(noteq,           "!=")
PUNCTUATOR(openpar,         "(")
PUNCTUATOR(plus,            "+")
PUNCTUATOR(semicolon,       ";")
PUNCTUATOR(slash,           "/")
PUNCTUATOR(star,            "*")

KEYWORD(BOOL,               KEYALL)
KEYWORD(ELSE,               KEYALL)
KEYWORD(FALSE,              KEYALL)
KEYWORD(FI,                 KEYALL)
KEYWORD(IF,                 KEYALL)
KEYWORD(INT,                KEYALL)
KEYWORD(PRINT,              KEYALL)
KEYWORD(RETURN,             KEYALL)
KEYWORD(THEN,               KEYALL)
KEYWORD(TRUE,               KEYALL)
KEYWORD(VAR,                KEYALL)

#undef KEYWORD
#undef PUNCTUATOR
#undef TOK
